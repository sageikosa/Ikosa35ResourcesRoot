<Page
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <Canvas Width="96" Height="96" Background="Violet">
        <Rectangle Canvas.Left="44" Canvas.Top="16" Stroke="Black" Width="8" Height="50">
      <Rectangle.Fill>
        <LinearGradientBrush StartPoint="0,0.5" EndPoint="1,0.5">
          <GradientStop Offset="0" Color="Gray"></GradientStop>
          <GradientStop Offset="0.5" Color="Silver"></GradientStop>
          <GradientStop Offset="1" Color="Gray"></GradientStop>
        </LinearGradientBrush>
      </Rectangle.Fill>
    </Rectangle>
    <Rectangle Canvas.Left="43" Canvas.Top="64" Stroke="Black" Fill="Brown" Width="10" Height="30"/>
    <Path Stroke="Black" Fill="LightGray" Data="M48,4 l2,14 l14,2 l-14,2 l-2,14 l-2,-14 l-14,-2 l14,-2 z"/>
    <Path Stroke="Black" Fill="LightGray" Data="M48,4 l2,14 l14,2 l-14,2 l-2,14 l-2,-14 l-14,-2 l14,-2 z">
          <Path.RenderTransform>
            <RotateTransform Angle="45" CenterX="48" CenterY="20"/>
          </Path.RenderTransform>
    </Path>
    <Ellipse Width="20" Height="20" Canvas.Left="38" Canvas.Top="10" Stroke="Black">
      <Ellipse.Fill>
        <RadialGradientBrush>
          <GradientStop Offset="0" Color="#FFe0e0e0"></GradientStop>
          <GradientStop Offset="1" Color="#FFa0a0a0"></GradientStop>
        </RadialGradientBrush>
      </Ellipse.Fill>
    </Ellipse>
    <Ellipse Fill="LightGray" Width="4" Height="4" Canvas.Left="46" Canvas.Top="18" Stroke="Black"></Ellipse>
  </Canvas>
</Page>