<DataTemplate xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<uv:ResourceContent xmlns:uv="clr-namespace:Uzi.Visualize;assembly=Uzi.Visualize" ResourceDictionary="{DynamicResource dictStyles}">
		<Canvas Background="{x:Null}" Width="96" Height="96" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
			<Rectangle Stroke="{uv:ColorBrushVal Key=Outline,Color=#FF000000}" Width="57" Height="80" Canvas.Left="19" Canvas.Top="8">
				<Rectangle.Fill>
					<LinearGradientBrush StartPoint="0,0.5" EndPoint="0.0714285,0.5" SpreadMethod="Reflect">
						<LinearGradientBrush.GradientStops>
							<GradientStop Color="{uv:ColorVal Key=SlatEdge,Color=#FF000000}" Offset="0" />
							<GradientStop Color="{uv:ColorVal Key=SlatEdge,Color=#FF000000}" Offset="0.125" />
							<GradientStop Color="{uv:ColorVal Key=Material,Color=#FF993300}" Offset="0.125" />
							<GradientStop Color="{uv:ColorVal Key=Material,Color=#FF993300}" Offset="0.25" />
						</LinearGradientBrush.GradientStops>
					</LinearGradientBrush>
				</Rectangle.Fill>
			</Rectangle>
			<Rectangle Fill="{uv:ColorBrushVal Key=Latch,Color=#FFA9A9A9}" Stroke="{uv:ColorBrushVal Key=Outline,Color=#FF000000}" Width="10" Height="10" Canvas.Left="66" Canvas.Top="43" />
			<Rectangle Fill="{uv:ColorBrushVal Key=Handle,Color=#FFC0C0C0}" Stroke="{uv:ColorBrushVal Key=Outline,Color=#FF000000}" Width="9" Height="3" Canvas.Left="64" Canvas.Top="46" />
		</Canvas>
	</uv:ResourceContent>
</DataTemplate>
