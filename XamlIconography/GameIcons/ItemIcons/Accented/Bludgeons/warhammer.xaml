<DataTemplate xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<uv:ResourceContent xmlns:uv="clr-namespace:Uzi.Visualize;assembly=Uzi.Visualize" ResourceDictionary="{DynamicResource dictStyles}">
		<Canvas Background="{x:Null}" Tag="R" Width="96" Height="96" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
			<Rectangle Stroke="{uv:ColorBrushVal Key=Outline,Color=#FF000000}" Width="8" Height="50" Canvas.Left="44" Canvas.Top="16">
				<Rectangle.Fill>
					<LinearGradientBrush StartPoint="0,0.5" EndPoint="1,0.5">
						<LinearGradientBrush.GradientStops>
							<GradientStop Color="{uv:ColorVal Key=Shaft2,Color=#FF808080}" Offset="0" />
							<GradientStop Color="{uv:ColorVal Key=Shaft1,Color=#FFC0C0C0}" Offset="0.5" />
							<GradientStop Color="{uv:ColorVal Key=Shaft2,Color=#FF808080}" Offset="1" />
						</LinearGradientBrush.GradientStops>
					</LinearGradientBrush>
				</Rectangle.Fill>
			</Rectangle>
			<Rectangle Fill="{uv:ColorBrushVal Key=Grip,Color=#FFA52A2A}" Stroke="{uv:ColorBrushVal Key=Outline,Color=#FF000000}" Width="10" Height="30" Canvas.Left="43" Canvas.Top="64" />
			<Rectangle Fill="{uv:ColorBrushVal Key=Neck,Color=#FF606060}" Stroke="{uv:ColorBrushVal Key=Outline,Color=#FF000000}" Width="20" Height="12" Canvas.Left="38" Canvas.Top="16" />
			<Rectangle Fill="{uv:ColorBrushVal Key=Cheek,Color=#FF606060}" Stroke="{uv:ColorBrushVal Key=Outline,Color=#FF000000}" Width="12" Height="16" Canvas.Left="42" Canvas.Top="14" />
			<Rectangle RadiusX="2" RadiusY="4" Fill="{uv:ColorBrushVal Key=Face,Color=#FF606060}" Stroke="{uv:ColorBrushVal Key=Outline,Color=#FF000000}" Width="12" Height="20" Canvas.Left="27" Canvas.Top="12" />
			<Rectangle RadiusX="2" RadiusY="4" Fill="{uv:ColorBrushVal Key=Face,Color=#FF606060}" Stroke="{uv:ColorBrushVal Key=Outline,Color=#FF000000}" Width="12" Height="20" Canvas.Left="57" Canvas.Top="12" />
		</Canvas>
	</uv:ResourceContent>
</DataTemplate>
