<Page
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <Canvas Height="96" Width="96" Background="Violet">  
    <Path Stroke="Black" StrokeThickness="1" Fill="Gold">
      <Path.Data>
        <GeometryGroup FillRule="EvenOdd">
          <EllipseGeometry RadiusX="26" RadiusY="24" Center="48,48"/>
          <EllipseGeometry RadiusX="16" RadiusY="20" Center="48,48"/>
        </GeometryGroup>
      </Path.Data>
    </Path>
  </Canvas>
</Page>